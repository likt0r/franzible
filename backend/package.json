    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run build && npm run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
